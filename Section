public class Person(){
private String name;
private int age;
public String getName{//获取name属性的getter方法
return name;
}
public String setName(String name){//设置name属性的setter方法
this.name=name;
}
public int getAge{//获取age属性的getter方法
return age;
}
public String setAge(int age){//设置age属性的setter方法
this.age=age;
}
}
